<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CarparkInformationBottom">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web_semibold"
        android:text="Address: "
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/address"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web"
        android:text="Address Unavailable"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:ellipsize="end"
        android:maxWidth="300dp"
        android:scrollHorizontally="false"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web_semibold"
        android:text="Carpark Type: "
        android:textColor="@color/black"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/carpark_type"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web"
        android:text="Carpark Type Unavailable"
        android:textSize="16sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7" />

    <TextView
        android:id="@+id/parking_system_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web_semibold"
        android:text="Parking System: "
        android:textColor="@color/black"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/parking_system"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web"
        android:text="Parking System Unavailable"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/parking_system_header"
        app:layout_constraintStart_toEndOf="@+id/parking_system_header"
        app:layout_constraintTop_toTopOf="@+id/parking_system_header" />

    <TextView
        android:id="@+id/night_parking_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web_semibold"
        android:text="Night Parking: "
        android:textColor="@color/black"
        android:textSize="16sp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="@+id/parking_system_header"
        app:layout_constraintTop_toBottomOf="@+id/parking_system_header" />

    <TextView
        android:id="@+id/night_parking"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web"
        android:text="Information Unavailable"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/night_parking_header"
        app:layout_constraintStart_toEndOf="@+id/night_parking_header"
        app:layout_constraintTop_toTopOf="@+id/night_parking_header" />

    <TextView
        android:id="@+id/gantry_height_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web_semibold"
        android:text="Gantry Height: "
        android:textColor="@color/black"
        android:layout_marginTop="40dp"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/night_parking_header"
        app:layout_constraintTop_toBottomOf="@+id/night_parking_header" />

    <TextView
        android:id="@+id/gantry_height"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web"
        android:text="Information Unavailable"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/gantry_height_header"
        app:layout_constraintStart_toEndOf="@+id/gantry_height_header"
        app:layout_constraintTop_toTopOf="@+id/gantry_height_header" />

    <TextView
        android:id="@+id/free_parking_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web_semibold"
        android:text="Free Parking: "
        android:textColor="@color/black"
        android:textSize="16sp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="@+id/gantry_height_header"
        app:layout_constraintTop_toBottomOf="@+id/gantry_height_header" />

    <TextView
        android:id="@+id/free_parking"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web"
        android:text="Information Unavailable"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/free_parking_header"
        app:layout_constraintStart_toEndOf="@+id/free_parking_header"
        app:layout_constraintTop_toTopOf="@+id/free_parking_header" />

    <TextView
        android:id="@+id/carpark_rates_for_hdb_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/titillium_web_semibold"
        android:text="Rates: "
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/free_parking_header"
        app:layout_constraintTop_toBottomOf="@+id/free_parking_header" />

    <TextView
        android:id="@+id/carpark_rates_for_hdb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/titillium_web"
        android:text="Rates Unavailable"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/carpark_rates_for_hdb_header"
        app:layout_constraintTop_toTopOf="@+id/carpark_rates_for_hdb_header" />


</androidx.constraintlayout.widget.ConstraintLayout>